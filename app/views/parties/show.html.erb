<div class="row map-form">
  <div class="col-md-6">
    <div class="form-group">
      Started at
      <div class='form-control'>
        <%= @party.start_at %>
      </div>
    </div>
    <div class="form-group">
      End at
      <div class='form-control'>
        <%= @party.end_at %>
      </div>
    </div>
    <div class="form-group">
      Venue
      <div class='form-control'>
        <%= @party.venue %>
      </div>
    </div>
    <%= hidden_field_tag :party_lat, @party.lat %>
    <%= hidden_field_tag :party_lng, @party.lng %>
  </div>
  <div class="col-md-6" id="map"></div>
</div>

<script>
  $(document).ready(function() {
    lat = parseFloat($('#party_lat').val());
    lng = parseFloat($('#party_lng').val());
    initMap(lat, lng);
  });
</script>
